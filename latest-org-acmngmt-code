 public String getOrganizationDictionaries() throws UnsupportedEncodingException {

        Map<String, Object> responseObject = Maps.newHashMap();
        Set<String> errors = Sets.newHashSet();
        List<Dictionary> customDictionariesList = Lists.newArrayList();

        if (StringUtils.isNotEmpty(organizationId)) {
            Organization newOrganization = organizationDAO.getById(organizationId);
            Set<Organizationdictionary> organizationDictionarySet = newOrganization.getOrganizationDictionarySet();
            for (Organizationdictionary current : organizationDictionarySet) {
                Dictionary dictionary = current.getOrganizationDictionary();
                customDictionariesList.add(dictionary);
            }
            responseObject.put("success", true);
            responseObject.put("dictionaryList", customDictionariesList);
        } else {
            errors.add("Error Updating, Please refresh page...");
            responseObject.put("success", false);
            responseObject.put("errors", errors);
        }
        jsonStream = ResponseUtils.getJSONResponseStream(responseObject);

        return ResponseConstants.JSON_RESPONSE;
    }

    public String createCustomDictionaries() throws UnsupportedEncodingException {

        Map<String, Object> responseObject = Maps.newHashMap();
        Set<String> errors = Sets.newHashSet();

        if (StringUtils.isNotEmpty(organizationId) && StringUtils.isNotEmpty(dictionaryName)) {
            Dictionary newDictionary = new Dictionary();
            newDictionary.setName(dictionaryName);
            dictionaryDAO.create(newDictionary);
            Organizationdictionary organizationdictionary = new Organizationdictionary(new OrganizationdictionaryId(newDictionary.getId(),
                    organizationId), newDictionary);
            organizationDictionaryDAO.create(organizationdictionary);
            responseObject.put("success", true);
        } else {
            errors.add("Error Updating, Please refresh page...");
            responseObject.put("success", false);
            responseObject.put("errors", errors);
        }
        jsonStream = ResponseUtils.getJSONResponseStream(responseObject);

        return ResponseConstants.JSON_RESPONSE;
    }
